    <fields>
        <fullName>Annual_Contract_Value__c</fullName>
        <description>This field takes the opportunity amount as a sum of opportunity products priced at monthly rates to calculate an annualized contract value.</description>
        <formula>Amount * 12</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This value assumes your opportunity line items state prices at monthly subscription rates for a quantity of users.</inlineHelpText>
        <label>Annual Contract Value</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
