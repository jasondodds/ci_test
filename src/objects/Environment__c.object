    <validationRules>
        <fullName>Packaging_False</fullName>
        <active>true</active>
        <errorConditionFormula>Packaging__c == false &amp;&amp; NOT(ISBLANK( Namespace__c ))</errorConditionFormula>
        <errorDisplayField>Namespace__c</errorDisplayField>
        <errorMessage>You cannot specify a Namespace if this Environment is not specified as a Packaging Environment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Packaging_True</fullName>
        <active>true</active>
        <errorConditionFormula>Packaging__c == True &amp;&amp; ISBLANK(Namespace__c)</errorConditionFormula>
        <errorDisplayField>Namespace__c</errorDisplayField>
        <errorMessage>You must specify a Namespace for this Packaging Environment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Name_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED(Name))</errorConditionFormula>
        <errorMessage>Name cannot be changed after creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Namespace</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Packaging__c = True, ISBLANK(Namespace__c))</errorConditionFormula>
        <errorMessage>Namespace is required when packaging is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_No_Namespace</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Packaging__c = false, NOT(ISBLANK(Namespace__c)))</errorConditionFormula>
        <errorMessage>Cannot have namespace if environment is not packaging.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Parent_Environment</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISBLANK(Parent_Environment__c),Packaging__c = False,Production__c = False)</errorConditionFormula>
        <errorMessage>Environment must have parent unless packaging or production.</errorMessage>
    </validationRules>
